
export const opaque_core_api = @{}
  fromRandom() ::
    return this.fromKey @
      this._kdf0_random()

  fromContent(u8_buf) ::
    return this.fromKey @
      this._kdf0_hash(u8_buf)

  async fromKey(key0) ::
    const k1ref = await this._okdf_ref(key0)
    const k2loc = await this._okdf_loc(k1ref.key)
    return @{} __proto__: this.key_proto, k1ref, k2loc

