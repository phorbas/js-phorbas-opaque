export { utf8_to_u8, u8_to_hex, hex_to_u8 } from 'u8-utils'

export function show_okey(okey) ::
  if okey.k1ref ::
    console.log(`
    expect(okey.k1ref)
    .to.deep.equal("${okey.hk1ref}")`)

  if okey.k1mid ::
    console.log(`
    expect(okey.k1mid)
    .to.deep.equal("${okey.hk1mid})"`)

  if okey.k2loc ::
    console.log(`
    expect(okey.k2loc)
    .to.deep.equal("${okey.hk2loc}")`)

