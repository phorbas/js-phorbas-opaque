
export function show_okey(okey) ::
  if okey.k1ref ::
    console.log(`
    expect(okey.k1ref)
    .to.deep.equal @ hex_to_u8 @ "${u8_to_hex(okey.k1ref)}"`)

  if okey.k1mid ::
    console.log(`
    expect(okey.k1mid)
    .to.deep.equal @ hex_to_u8 @ "${u8_to_hex(okey.k1mid)}"`)

  if okey.k2loc ::
    console.log(`
    expect(okey.k2loc)
    .to.deep.equal @ hex_to_u8 @ "${u8_to_hex(okey.k2loc)}"`)

